<!DOCTYPE html>
<html>

<head>
    <title>StudyMate | Find A Study Partner</title>
    <!-- Favicon -->
    <link rel="icon" href="assets/images/favicon.ico">
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <%- include('usercommonCSSJS') -%>
</head>

<body>
    <div class="container-fluid">
        <%- include('userheader') -%>
            <section class="mt-55" id="findstudybuddy">
                <!-- <div class="container-fluid"> -->
                <hr class="featurette-divider">
                <div class="row featurette">
                    <%- include('filterpanel') -%>
                        <div class="col-md-9" id="mainpanel">
                            <div class="row">
                                <h2 class="featurette-heading"><span class="text-muted">Results</span></h2>
                                <div class="form-outline mt-4">
                                    <span class="successMsgResult"></span>
                                    <span class="errorMsgResult"></span>
                                </div>
                            </div>
                            <div class="row profiletiles" id="filteringResults">
                                <% if(Object.keys(studentInfo).length> 0) {
                                    Object.entries(studentInfo).forEach(([key, value]) => {
                                    %>
                                <div class="col-md-3 profiletile" user-id="<%= value.ru_id %>">
                                    <div class="col-md-12 profileimage"><img
                                            src="../assets/images/userProfile/<%= value.ru_id %>.jpg"
                                            alt="profile picture" class="img-fluid"></div>
                                    <div class="col-md-12 profiletitle"><a class="" target="_blank"
                                            href="view-student-profile/<%= value.ru_id %>"><%= value.name %></a></div>
                                    <div class="col-md-12" style="text-align: center;"><button
                                            class="btn btn-danger btn-sm btn-block sendrequets">Send Request</button>
                                    </div>
                                </div>
                                <% }); } else { %>
                                    <div class="col-md-12">
                                        <span class="errorMsgResult">No results available</span>
                                    </div>
                                <%  } %>
                            </div>
                            <div class="row" id="recommendationResults">
                                <h2 class="featurette-heading"><span class="text-muted">Recommendations</span></h2>
                                <div id="carouselExampleControls" class="carousel slide" data-ride="carousel">
                                    <div class="carousel-inner">
                                        <div class="carousel-item active">
                                            <div class="row recommendationtiles">
                                                <div class="col-md-2 profiletile">
                                                    <div class="col-md-12 profileimage">
                                                        <img src="../assets/images/userProfile/male.jpg"
                                                            class="img-thumbnail" alt="profile pictures">
                                                    </div>
                                                    <div class="col-md-12 profiletitle">
                                                        <a class="" href="#">Student Name</a>
                                                    </div>
                                                </div>
                                                <div class="col-md-2 profiletile">
                                                    <div class="col-md-12 profileimage">
                                                        <img src="../assets/images/userProfile/male.jpg"
                                                            alt="profile picture" class="img-fluid">
                                                    </div>
                                                    <div class="col-md-12 profiletitle">
                                                        <a class="" href="#">Student Name</a>
                                                    </div>
                                                </div>
                                                <div class="col-md-2 profiletile">
                                                    <div class="col-md-12 profileimage">
                                                        <img src="../assets/images/userProfile/male.jpg"
                                                            alt="profile picture" class="img-fluid">
                                                    </div>
                                                    <div class="col-md-12 profiletitle">
                                                        <a class="" href="#">Student Name</a>
                                                    </div>
                                                </div>
                                                <div class="col-md-2 profiletile">
                                                    <div class="col-md-12 profileimage">
                                                        <img src="../assets/images/userProfile/male.jpg"
                                                            alt="profile picture" class="img-fluid">
                                                    </div>
                                                    <div class="col-md-12 profiletitle">
                                                        <a class="" href="#">Student Name</a>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="carousel-item">
                                            <div class="row recommendationtiles">
                                                <div class="col-md-2 profiletile">
                                                    <div class="col-md-12 profileimage">
                                                        <img src="../assets/images/userProfile/female.jpg"
                                                            alt="profile picture" class="img-fluid">
                                                    </div>
                                                    <div class="col-md-12 profiletitle">
